.react-toggle-track
  border-radius 3px !important
  
.react-toggle-thumb
  border-radius 3px !important
  width 26px !important
  height 26px !important
  top -1px !important
  
.react-toggle-track-x, .react-toggle-track-check
  display none !important
  
.react-toggle .react-toggle-track
  background-color #f9fbfd !important
  border 1px solid #aab5c1 !important

.react-toggle--checked .react-toggle-track
  background-color #00A4BE !important
  border 1px solid #00A4BE !important
  
.react-toggle .react-toggle-thumb
  border-color #afb9c4 !important
  margin-left -1px !important
  
.react-toggle--checked .react-toggle-thumb
  border-color #00A4BE !important