.log .log__line:nth-child(even)
  background rgba(0,0,0,0.025)

pre.log
  padding 0
  background #f8f8f8
  margin-top 20px
  background #f6f6f6
  background-color #f5f5f5
  border none
  border-radius 0


.log__line
  font-family Consolas, "Liberation Mono", Courier, monospace
  padding 4px 10px
  white-space pre